a_start () := -100 (const, value=-100) # Store array a start index
L2: # MAIN_START: Main program start
read x () # Read value into x
t1 (temp) := -100 (const, value=-100) - a_start () # Calculate array offset for assignment
a (array[-100:-80])[t1 (temp)] := 100 (const, value=100) # Write to array a[-100]
t3 (temp) := -100 (const, value=-100) - a_start () # Calculate array a offset
t2 (temp) := a (array[-100:-80])[t3 (temp)] # Read from array a[-100]
write t2 (temp) # Write value t2
