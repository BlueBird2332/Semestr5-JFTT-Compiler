PROCEDURE swap(T arr, i, j) IS
BEGIN
    temp := arr[i];
    arr[i] := arr[j];
    arr[j] := temp;
END

PROGRAM IS
    arr[1:10], i, j, n
BEGIN
    n := 10;
    i := 1;
    WHILE i <= n DO
        READ arr[i];
        i := i + 1;
    ENDWHILE

    i := 1;
    WHILE i < n DO
        j := i + 1;
        WHILE j <= n DO
            IF arr[i] > arr[j] THEN
                swap(arr, i, j);
            ENDIF
            j := j + 1;
        ENDWHILE
        i := i + 1;
    ENDWHILE

    i := 1;
    WHILE i <= n DO
        WRITE arr[i];
        i := i + 1;
    ENDWHILE
END