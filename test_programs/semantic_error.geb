PROGRAM IS
    a, b, tab[10:5]
BEGIN
    a := -42;
    b := -10;
    
    # Negative array index
    tab[-3] := -4;  # Should be -52
    
    # Expression with negatives
    tab[-2] := -2;  # Should be 84
    
    # Mixed operations
    tab[-1] := - 2;   # Should be 5
    tab[0] := -5;    # Should be -2
    
    # Print results
    WRITE tab[-3];  # -52
    WRITE tab[-2];  # 84s
    WRITE tab[-1];  # 5
    WRITE tab[0];   # -2
END